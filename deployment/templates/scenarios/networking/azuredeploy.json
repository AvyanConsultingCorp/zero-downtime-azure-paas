{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentReference": {
      "type": "secureObject"
    },
    "frontEndTier": {
      "type": "secureObject"
    },
    "middleTier": {
      "type": "secureObject"
    },
    "backEndTier": {
      "type": "secureObject"
    },
    "operations": {
      "type": "secureObject"
    },
    "network": {
      "type": "secureObject"
    }
  },
  "variables": {
    "trafficManagerName": "[toLower(concat(parameters('environmentReference').prefix,'-',parameters('network').trafficManager.serviceName,'-tfm'))]"
  },
  "resources": [
    { 
      "apiVersion": "2017-05-10", 
      "name": "deploy-trafficmanager", 
      "type": "Microsoft.Resources/deployments",
      "dependsOn": [],
      "properties": { 
         "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('environmentReference')._artifactsLocation,'templates/resources/microsoft.network/trafficmanager.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": { 
        "name":{"value": "[variables('trafficManagerName')]"}
        }
      }
    }
  ],
  "outputs": {}
}